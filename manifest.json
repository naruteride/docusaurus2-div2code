{
    "name": "docusaurus2 div2code",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "icon"
    },
    "background": {
        "scripts": ["background.js"] 
    },
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "js": ["content-script.js"]
        }
    ]
}